<!doctype html>
<html>

<head>
    <title>DentaQuery</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="search.css">
    <link rel="stylesheet" href="pen.css">
</head>

<body>
   
    <!---------------------- NAVBAR ------------------------------->
    <nav id="gradient-custom" class="navbar navbar-expand-md bg-light navbar-dark">
        <div class="container">

            <a href="/" class="navbar-brand"><img id="navLogo" src="img/default-monochromeNoMoto.svg"
                    alt="DentaQuery Logo"></a>

            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navmenu">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navmenu">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item">
                        <a href="/search" class="nav-link">Search</a>
                    </li>
                    <!-- <li class="nav-item">
                        <a href="/query" class="nav-link">Query</a>
                    </li> -->
                    <li class="nav-item">
                        <a href="/commonQuestions" class="nav-link">Common Questions & Tools</a>
                    </li>
                    <li class="nav-item">
                        <a href="/login" class="nav-link">Login</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
<!---------------------- PAGE CONTENT ------------------------------->
    <div class="searchPage">
       <div class="searchField">
           <form onsubmit="return false">
                <input class="mainSearchBox" placeholder="Search procedure code or procedure description..." type="text" id="searchBar" autofocus="autofocus" onfocus="this.select()">

                <button type="submit"  class="btn-primary" id="searchBtn">Search</button>
           </form>  
       </div>
          
       <div class="searchResults">
        <ul id="searchList">
        </ul>
       </div>
        
    </div>

    <section class="footer" id="gradient-custom">
    </section>

    <script src="https://kit.fontawesome.com/c0e668a8ad.js" crossorigin="anonymous"></script>
    <script src="main.js"></script>

</body>

</html>










